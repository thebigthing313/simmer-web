{
	"deno.enable": true,
	"deno.enablePaths": ["./functions"],
	"deno.lint": true,
	"deno.unstable": [
		"bare-node-builtins",
		"byonm",
		"sloppy-imports",
		"unsafe-proto",
		"webgpu",
		"broadcast-channel",
		"worker-options",
		"cron",
		"kv",
		"ffi",
		"fs",
		"http",
		"net"
	],
	"editor.formatOnSave": true,
	"[typescript]": {
		"editor.defaultFormatter": "denoland.vscode-deno"
	},
	"[javascript]": {
		"editor.defaultFormatter": "denoland.vscode-deno"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "denoland.vscode-deno"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "denoland.vscode-deno"
	}
}
